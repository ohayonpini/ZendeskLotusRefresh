{
  "name": "Injects a refresh event for Zendesk New \"Lotus\" interface. v1.1 (3/26/2013)",
  "version": "1.1",
  "permissions": [
    "https://*.zendesk.com/agent/*"
  ],
  "icons":{"128": "refresh-icon.png"},
  "web_accessible_resources": [
	"script.js"],
    "content_scripts": [
    {
      "matches": ["https://*.zendesk.com/agent/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
